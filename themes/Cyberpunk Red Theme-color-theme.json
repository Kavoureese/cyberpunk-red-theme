{
	"name": "Cyberpunk Red Theme",
	"type": "dark",
	"colors": {
		// VS Code
			// Title Bar (Top Bar)
			"titleBar.activeBackground": "#140009",
			"titleBar.activeForeground": "#ff8ba8",
			"titleBar.inactiveBackground": "#140009",
			"titleBar.inactiveForeground": "#ff8ba8",

			// Activity Bar (Left-Most Bar)
			"activityBarBadge.background": "#35ffff",
			"activityBarBadge.foreground": "#140009",
			"activityBar.background": "#060608",
			"activityBar.foreground": "#ff8ba8",
			"activityBar.inactiveForeground": "#ff8ba8",

			// Side Bar (Folder view, right next to Activity Bar)
			"sideBar.background": "#0a0b0e",
			"sideBar.border": "#101116",
			"sideBar.foreground": "#ff8ba8",
			"sideBarTitle.foreground": "#bbbbbb",
			"sideBarSectionHeader.background": "#240000",
			"sideBarSectionHeader.border": "#ff0055",
			"sideBarSectionHeader.foreground": "#ff8ba8",

			// Line Number Column
			"editorGutter.background": "#060608",

			// Status Bar (Bottom Bar)
			"statusBar.background": "#140009",
			"statusBar.foreground": "#ff8ba8",
			"statusBar.debuggingBackground": "#ff005580",
			"statusBar.debuggingForeground": "#35ffff",
			"statusBar.noFolderBackground": "#140009",
			"statusBar.noFolderForeground": "#ff8ba8",

			// Debug toolbar
			"debugToolBar.background": "#140009",
			"debugToolBar.border": "#ff0055",

			// Badges
			"badge.background": "#1d000ae5",
			"badge.foreground": "#ff0055",

			// Buttons
			"button.border": "#ff8ba8",
			"button.background": "#140009",
			"button.foreground": "#ff8ba8",
			"button.hoverBackground": "#2e0015",
			"button.secondaryBackground": "#140009",
			"button.secondaryForeground": "#ff8ba8",
			"button.secondaryHoverBackground": "#2e0015",

			// List Dropdown (Command palette etc)
			"list.activeSelectionBackground": "#140009",
			"list.hoverBackground": "#140009",
			"list.activeSelectionForeground": "#ff8ba8",
			"list.hoverForeground": "#ff8ba8",

			// Terminal
			"terminalCursor.foreground": "#35ffff",
			"terminal.background": "#060608",

			// Git Decorations
			"gitDecoration.addedResourceForeground": "#ff0055",
			"gitDecoration.modifiedResourceForeground": "#ff8ba8",
			"gitDecoration.untrackedResourceForeground": "#35ffff",
			"gitDecoration.ignoredResourceForeground": "#906565",
			
			"editorGutter.addedBackground": "#ff005580",
			"editorGutter.modifiedBackground": "#ff8ba8",
			"editorGutter.deletedBackground": "#35ffff",
			
			"editorOverviewRuler.addedForeground": "#ff0055",
			"editorOverviewRuler.modifiedForeground": "#ff8ba880",
			"editorOverviewRuler.deletedForeground": "#35ffff80",

			// Squiggly Lines (Errors and Warnings)
			"editorError.background": "#ff8ba850",
			"editorError.foreground": "#ff8ba8",
			"editorWarning.foreground": "#fcee0a",

			"minimap.findMatchHighlight": "#00ffc8",
			"minimapGutter.addedBackground": "#ff0055",
			"minimapGutter.modifiedBackground": "#ff8ba8",
			"minimapGutter.deletedBackground": "#35ffff",

		// Editor (Code panel)
		"editor.background": "#060608",
		"editor.foreground": "#35ffff",
		"editor.findMatchHighlightBackground": "#00000000",
		"editor.findMatchHighlightBorder": "#35ffff",
		"editorLink.activeForeground": "#ff0055",

			// Cursor
			"editorCursor.foreground": "#35ffff",

			// Input fields
			"input.background": "#140009",
			"input.border": "#ff8ba8",
			"input.foreground": "#ff8ba8",
			"input.placeholderForeground": "#ff8ba8",
			"inputOption.activeBackground": "#140009",
			"inputOption.activeBorder": "#00ffc8",
			"inputOption.activeForeground": "#00ffc8",
			"inputOption.hoverBackground": "#140009",

			// Widgets (Find/Replace etc)
			"editorWidget.background": "#1d000ae5",
			"editorWidget.border": "#ff0055",

			// Selection
			"editor.selectionBackground": "#ff000041",

			// Dropdown	(Panel shown when right-click)
			"dropdown.background": "#1d000ae5",
			"dropdown.border": "#ff8ba8",
			"dropdown.foreground": "#ff8ba8",

			// Editor Line Numbers
			"editorLineNumber.foreground": "#ff00557e",
			"editorLineNumber.activeForeground": "#35ffff",

			// Panels (Terminal, etc)
			"panel.background": "#101116",
			"panel.border": "#ff0055",
			"panelTitle.activeBorder": "#ff0055",
			"panelTitle.activeForeground": "#ff0055",
			"panelTitle.inactiveForeground": "#eb4d81de",

			// Scroll Bar
			"scrollbarSlider.background": "#ff000041",
			"scrollbarSlider.activeBackground": "#ff0055",
			"scrollbarSlider.hoverBackground": "#ff005578",

			// Tabs
			"tab.activeBackground": "#151621",
			"tab.inactiveBackground": "#060608",
			"tab.activeBorder": "#35ffff",
			"tab.hoverBackground": "#151621",
			"tab.hoverBorder": "#35ffff",
			"tab.border": "#101116",
			"tab.activeForeground": "#35ffff",
			"tab.inactiveForeground": "#eb4d81de",
			"editorGroupHeader.tabsBackground": "#060608",
			"editorGroupHeader.noTabsBackground": "#060608",

			// TODO: Figure out nested bracket colors
			"editorBracketHighlight.foreground1": "#ff0055",
			"editorBracketHighlight.foreground2": "#ff0055",
			"editorBracketHighlight.foreground3": "#ff0055",
			"editorBracketHighlight.foreground4": "#ff0055",
			"editorBracketHighlight.foreground5": "#ff0055",
			"editorBracketHighlight.foreground6": "#ff0055",
			"editorBracketHighlight.unexpectedBracket.foreground": "#fcee0a",

			// Bracket Pairs
			// NOTE: Requires enabling "Bracket Pairs" in settings
			"editorBracketMatch.background": "#35ffff",
			"editorBracketMatch.border": "#ff000000",
			"editorBracketPairGuide.activeBackground1": "#ff0055",
			"editorBracketPairGuide.background1": "#ff8ba850",
			"editorBracketPairGuide.activeBackground2": "#ff0055",
			"editorBracketPairGuide.background2": "#ff8ba850",
			"editorBracketPairGuide.activeBackground3": "#ff0055",
			"editorBracketPairGuide.background3": "#ff8ba850",
			"editorBracketPairGuide.activeBackground4": "#ff0055",
			"editorBracketPairGuide.background4": "#ff8ba850",
			"editorBracketPairGuide.activeBackground5": "#ff0055",
			"editorBracketPairGuide.background5": "#ff8ba850",
			"editorBracketPairGuide.activeBackground6": "#ff0055",
			"editorBracketPairGuide.background6": "#ff8ba850"
	},
	"tokenColors": [
		{
			"name": "Comments and preprocessor",
			"scope": [
				"comment",
				"comment.block",
				"comment.block.documentation",
				"comment.line"
			],
			"settings": {
				"foreground": "#00ffc8",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Preprocessor definitions and other",
			"scope": [
				"entity.name.function.preprocessor",
				"support.type.property-name.json.comments",		// JSON field name
				"variable.other.member.c"						// struct.member
			],
			"settings": {
				"foreground": "#8a84e4"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "#b9003e"
			}
		},
		{
			"name": "Keywords,Operator Keywords, Variable Types, Structs",
			"scope": [
				"keyword",
				"keyword.control",
				"storage.modifier",
				"storage.type",
				"entity.name.type.c",						// Struct
				"punctuation.separator.dot-access.c",		// '.' struct member access
				"punctuation.separator.pointer-access.c",	// '->' struct pointer member access

				// Markdown
				"heading.1.markdown",
				"heading.2.markdown",
				"heading.3.markdown",
				"heading.4.markdown",
				"heading.5.markdown",
				"heading.6.markdown",
				"punctuation.definition.link.description.begin.markdown",
				"punctuation.definition.link.description.end.markdown",
				"punctuation.definition.link.title.begin.markdown",
				"punctuation.definition.link.title.end.markdown",
				"punctuation.definition.metadata.markdown",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"entity.name.tag.html",
				"punctuation.definition.strikethrough.markdown",
				"punctuation.definition.bold.markdown",
				"punctuation.definition.italic.markdown"
			],
			"settings": {
				"foreground": "#ff0055"
			}
		},
		{
			"name": "Markdown strikethrough",
			"scope": [
				"markup.strikethrough.markdown"
			],
			"settings": {
				"fontStyle": "strikethrough",
				"foreground": "#a0d3ff"
			}
		},
		{
			"name": "Markdown italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#a0d3ff",
			}
		},
		{
			"name": "Markdown bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#a0d3ff"
			}
		},
		{
			"name": "Numbers, Literals, Constants",
			"scope": [
				"constant",
				"keyword.other.unit.exponent.decimal.c",		// 'e' exponent keyword
				"keyword.operator.minus.exponent.decimal.c",	// '-' in floating point literals
				"keyword.other.unit.binary.c",					// '0b' prefix for binary numbers
				"keyword.other.unit.suffix.floating-point.c",	// 'f' suffix for f32 numbers
				"keyword.other.unit.octal.c",					// '0' prefix for octal numbers
				"keyword.other.unit.suffix.integer",			// 'U' suffix
				"keyword.other.unit.hexadecimal"				// '0x' prefix
			],
			"settings": {
				"foreground": "#b7faff"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"constant.character",

				// Markdown
				"string.other.link.description.markdown"
			],
			"settings": {
				"foreground": "#a0d3ff"
			}
		},
		{
			"name": "String Placeholders (printf identifiers)",
			"scope": [
				"constant.other.placeholder",

				// Markdown
				"markup.underline.link.image.markdown",
				"markup.underline.link.markdown"
			],
			"settings": {
				"foreground": "#00ffc8"
			}
		}
	]
}
